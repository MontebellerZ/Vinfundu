<p id="notice"><%= notice %></p>

<h1>Sessaos</h1>

<table>
  <thead>
    <tr>
      <th>Cinema</th>
      <th>Filme</th>
      <th>Inicio</th>
      <th>Sala</th>
      <th>Tipo</th>
      <th>Cadeiras</th>
      <th>Valor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessaos.each do |sessao| %>
      <tr>
        <td><%= sessao.cinema_id %></td>
        <td><%= sessao.filme_id %></td>
        <td><%= sessao.inicio %></td>
        <td><%= sessao.sala %></td>
        <td><%= sessao.tipo %></td>
        <td><%= sessao.cadeiras %></td>
        <td><%= sessao.valor %></td>
        <td><%= link_to 'Show', sessao %></td>
        <td><%= link_to 'Edit', edit_sessao_path(sessao) %></td>
        <td><%= link_to 'Destroy', sessao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sessao', new_sessao_path %>
